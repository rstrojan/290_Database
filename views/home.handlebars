<h1>hello</h1>
{{results}}

<table style="width:100%" id="tracker">
  <tr>
    <th>Name</th>
    <th>Reps</th>
    <th>Weight</th>
	<th>lbs</th>
	<th>Date</th>
	<th></th>
  </tr>
  <form>
  <tr>
	<td><input type="text" name="Name" id="add_name" size="30" maxlength="30">
	<td><input type="number" name="Reps" id="add_reps" value="1" min="1">
	<td><input type="number" name="Weight" id="add_weight" value="1" min="1">
	<td><input list="lbs" name="lbs" id="add_lbs">
				<datalist id="lbs">
					<option value="true">
					<option value="false">
				</datalist>
	<td><input type="date" name="Entry Date" id="add_date" value="2019-11-24" size="20">
	<td><input type="button" value ="Add Entry" onclick="addRow('tracker')"/>
  </tr>
  </form>
  {{#each dataList}}
  <tr>
    <td>{{this.name}}
	<td>{{this.reps}}
	<td>{{this.weight}}
	<td>{{this.lbs}}
	<td>{{this.date}}
	<td><input type="button" value="Remove Entry" onclick="deleteRow('tracker',this, {{this.id}})" />
  </tr>
  {{/each}}
</table>

<script src="/js/tableEdit.js"></script>